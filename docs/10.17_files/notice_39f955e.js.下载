window.TTNotice=function(){function e(e,t,o,n,c,i,s,a){var p=s?"inline-block":"none",l=['<div class="popup-wrap">','<div class="popup-box">','<div class = "title">','<div class="warm-icon"></div>','<span class= "notice-title">',t,"</span>","</div>",'<div class= "content">',o,"</div>",'<div class="btn popup-close">',n,"</div>","</div>","</div>"].join(""),v=['<div class="popup-wrap">','<div class="popup-box">','<div class = "title">','<div class="success-icon"></div>','<span class= "notice-title">',t,"</span>","</div>",'<div class= "content">',o,"</div>",'<div class="btn popup-cancle" style="display:'+p+'">',"取消","</div>",'<div class="btn popup-close">',n,"</div>","</div>","</div>"].join("");this.tpl=1==c?l:v,this.$parent=e,this.$el=$(this.tpl),this.callback=i,this.canclecb=a,this.init(this.$el),this.bindEvent(this.$el)}var t=function(e){e.preventDefault(),e.stopPropagation()};return e.prototype={init:function(e){this.$parent.append(e),document.body.style.overflow="hidden",document.getElementsByTagName("body")[0].addEventListener("touchmove",t,!1)},bindEvent:function(e){var o=this;e.on("click",".popup-close",function(e){$(".popup-wrap").remove(),document.body.style.overflow="",document.getElementsByTagName("body")[0].removeEventListener("touchmove",t,!1),e.preventDefault(),e.stopPropagation(),o.callback&&o.callback()}),e.on("click",".popup-cancle",function(e){$(".popup-wrap").remove(),document.body.style.overflow="",document.getElementsByTagName("body")[0].removeEventListener("touchmove",t,!1),e.preventDefault(),e.stopPropagation(),o.canclecb&&canclecb()}),e.on("click",function(e){$(".popup-wrap").remove(),document.body.style.overflow="",document.getElementsByTagName("body")[0].removeEventListener("touchmove",t,!1),e.preventDefault(),e.stopPropagation()})}},{create:function(t,o){var n=o.title||"提示",c=o.content||"",i=o.buttonContent||"关闭",s=o.type||0,a=o.callback,p=o.confirm||!1,l=o.canclecb;return new e(t,n,c,i,s,a,p,l)}}}();
