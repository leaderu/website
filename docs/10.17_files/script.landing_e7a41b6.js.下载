define("tetris/parse/Map.js",function(n,i,a){var e={init:function(n,i,a){if($("#"+n).length&&a){var e=this;e.createTip(),e.initMap(n,i,a)}},initMap:function(n,i,a){var e=a.split(","),t=new BMap.Map(n),p=new BMap.Point(e[0],e[1]);t.centerAndZoom(p,15),t.disableDragging();var r=new BMap.Marker(p);t.addOverlay(r);var o=$("#"+n).parents().find(".map-con").eq(0),s=o.find(".overlay");s.bind("touchmove",function(n){var i=n.touches[0],a=50;$(".tinyTip").css({top:i.pageY-a,left:i.pageX-a}).show()}),s.bind("touchend",function(n){$(".tinyTip").hide()})},createTip:function(){var n=$("body"),i="tinyTip";if(!n.find("."+i).length){var a=$('<span class="'+i+'">点击进入地图</span>');n.append(a)}}};a.exports=e});
