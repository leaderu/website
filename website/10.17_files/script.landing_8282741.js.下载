define("tetris/parse/Slider.js",function(e,i,t){function n(e,i,t,n,o,s){this.$el=e,this.sliderComponent=this.$el[0],this.styleType=parseInt(t);var l="#"+this.sliderComponent.id+">.swiper-container",r=$(l),c=(r.parents(".slide-container"),$(window).height()),p=$(".swiper-slide").length;if(o)switch(o){case"4:3":r.height(.75*r.width());break;case"5:3":r.height(.6*r.width());break;case"16:9":r.height(.5625*r.width());break;case"3:1":r.height(.33*r.width());break;case"auto":r.css({height:"auto"}),2==this.styleType&&r.find(".swiper-slide img").css({"max-height":c});break;default:r.height(.75*r.width())}switch(r.css({visibility:"visible"}),this.styleType){case 0:new Swiper(l,{pagination:".swiper-pagination",loop:n,autoplay:!!i&&3e3,autoplayDisableOnInteraction:!1,onSliderMove:function(e,i){0==d&&(window.removeTetrisAction("slider","manualslide"),window.addTetrisAction({component_type:"slider",component_id:"",action_type:"manualslide",action_value:"1"})),d=1},onSlideChangeEnd:function(e){var i=$(".swiper-pagination-bullet-active").index();a.indexOf(i)==-1&&a.push(i);var t=a.length;window.removeTetrisAction("slider","pct"),window.addTetrisAction({component_type:"slider",component_id:"",action_type:"pct",action_value:t+"/"+p})}});break;case 1:r.find(".swiper-slide").addClass("style1"),new Swiper(l,{pagination:".swiper-pagination",slidesPerView:"auto",spaceBetween:10,autoplay:!!i&&3e3,autoplayDisableOnInteraction:!1,loop:n,onSliderMove:function(e,i){0==d&&(window.removeTetrisAction("slider","manualslide"),window.addTetrisAction({component_type:"slider",component_id:"",action_type:"manualslide",action_value:"1"})),d=1},onSlideChangeEnd:function(e){var i=$(".swiper-pagination-bullet-active").index();a.indexOf(i)==-1&&a.push(i);var t=a.length;window.removeTetrisAction("slider","pct"),window.addTetrisAction({component_type:"slider",component_id:"",action_type:"pct",action_value:t+"/"+p})}});break;case 2:r.find(".swiper-slide").addClass("style2");var u=function(){new Swiper(l,{scro